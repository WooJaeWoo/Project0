					<div id="clockNeedle2" class="clockFace">
						<img src="/img/q2_clockNeedle_2.png" alt="">
					</div>
					<div id="clockNeedle1" class="clockFace">
						<img src="/img/q2_clockNeedle_1.png" alt="">
					</div>
					<div id="age10" class="age" data-value="10"><span>10</span>대</div>
					<div id="age20" class="age" data-value="20"><span>20</span>대</div>
					<div id="age30" class="age" data-value="30"><span>30</span>대</div>
					<div id="age40" class="age" data-value="40"><span>40</span>대</div>
