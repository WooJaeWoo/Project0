					<div id="marriageButton">
						<img id="marriageButtonImg" src="/img/q3_default.png" alt="marriage button">
						<button class="marriageYes" data-value="yes"></button>
						<button class="marriageNo" data-value="no"></button>
					</div>
				</div>
			</div>
		</section>
